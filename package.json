{
  "name": "website-game-tv",
  "repository": {
    "type": "git",
    "url": "https://github.com/bluestacks/website-game-tv.git"
  },
  "version": "3.0.0",
  "description": "",
  "author": "",
  "license": "MIT",
  "scripts": {
    "webpack": "webpack --config webpack.config.js",
    "clean": "rimraf dist",
    "prebuild": "npm run clean",
    "build": "gulp --gulpfile gulpfile.js",
    "build:local": "NODE_ENV=local ENVIRONMENT=local npm run build",
    "build:development": "NODE_ENV=development ENVIRONMENT=development  npm run build",
    "build:production": "NODE_ENV=production ENVIRONMENT=production  npm run build",
    "build:debug": "NODE_ENV=debug ENVIRONMENT=debug npm run build",
    "build:staging": "NODE_ENV=staging ENVIRONMENT=staging npm run build",
    "prestart:local": "npm run build:local",
    "prestart:debug": "npm run build:debug",
    "prestart:development": "npm run build:development",
    "prestart:staging": "npm run build:staging",
    "prestart:production": "npm run build:production",
    "start:local": "NODE_ENV=local ENVIRONMENT=local concurrently \"nodemon --config nodemon-dev.json\"  \"gulp watch\"",
    "start:development": "NODE_ENV=development ENVIRONMENT=development concurrently \"nodemon --config nodemon-dev.json\"  \"gulp watch\"",
    "start:debug": "NODE_ENV=debug ENVIRONMENT=debug nodemon --config nodemon-debug.json",
    "start:staging": "NODE_ENV=staging ENVIRONMENT=staging nodemon --config nodemon.json",
    "start:production": "NODE_ENV=production ENVIRONMENT=production nodemon --config nodemon.json",
    "start": "nodemon --config nodemon.json",
    "clean:docker-local": "docker-compose -f docker-compose.local.yml rm --force",
    "start:docker-local": "docker-compose -f docker-compose.local.yml up --build --force-recreate",
    "format": "npm run format:code && npm run format:style",
    "format:js": "prettier --write './src/public/js/**/*.js'",
    "format:code": "prettier --write \"src/**/*.ts\"",
    "format:style": "stylelint './src/**/*.scss' --syntax scss --fix",
    "lint": "npm run lint:code && npm run lint:style",
    "lint:js": "prettier --check './src/public/js/**/*.js'",
    "lint:code": "tslint -p tsconfig.json -c tslint.json",
    "lint:style": "stylelint './src/**/*.scss' --syntax scss",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "migration:generate": "./node_modules/.bin/ts-node ./node_modules/.bin/typeorm migration:generate -n migration -d migrations",
    "migration:create": "./node_modules/.bin/ts-node ./node_modules/.bin/typeorm migration:create -n migration -d migrations",
    "migration:up": "./node_modules/.bin/ts-node -r tsconfig-paths/register ./node_modules/.bin/typeorm migration:run",
    "migration:down": "./node_modules/.bin/ts-node ./node_modules/.bin/typeorm migration:revert"
  },
  "dependencies": {
    "@bugsnag/js": "^6.4.3",
    "@maxmind/geoip2-node": "^1.3.0",
    "@nestjs/common": "^6.10.7",
    "@nestjs/core": "^6.10.7",
    "@nestjs/mongoose": "^6.2.1",
    "@nestjs/platform-express": "^6.10.7",
    "class-validator": "^0.11.0",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.4",
    "csurf": "^1.10.0",
    "dotenv": "^8.2.0",
    "express-handlebars": "^3.1.0",
    "express-robots-txt": "^0.4.1",
    "handlebars.moment": "^1.0.4",
    "helmet": "^3.21.2",
    "ioredis": "^4.14.1",
    "lodash": "^4.17.15",
    "memcached": "^2.2.2",
    "moment-timezone": "^0.5.27",
    "mongoose": "^5.8.7",
    "nest-schedule": "^0.6.4",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^6.5.3",
    "sitemap": "^5.1.0",
    "swiper": "^5.2.1"
  },
  "devDependencies": {
    "@nestjs/testing": "^6.10.7",
    "@types/compression": "1.0.1",
    "@types/cookie-parser": "^1.4.2",
    "@types/swiper": "^5.2.0",
    "@types/csurf": "^1.9.36",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.2",
    "@types/express-handlebars": "3.1.0",
    "@types/helmet": "0.0.45",
    "@types/jest": "^24.0.23",
    "@types/node": "^12.12.14",
    "@types/node-sass": "^4.11.0",
    "@types/supertest": "^2.0.8",
    "acorn": "^7.1.0",
    "autoprefixer": "^9.7.3",
    "browserify": "^16.5.0",
    "concurrently": "^5.0.0",
    "cssnano": "^4.1.10",
    "event-stream": "^4.0.1",
    "glob": "^7.1.6",
    "gulp": "^4.0.2",
    "gulp-buffer": "0.0.2",
    "gulp-postcss": "^8.0.0",
    "gulp-replace": "^1.0.0",
    "gulp-sass": "^4.0.2",
    "gulp-sourcemaps": "^2.6.5",
    "gulp-tap": "^2.0.0",
    "gulp-terser": "^1.2.0",
    "gulp-typescript": "^5.0.1",
    "gulp-typescript-path-resolver": "0.0.1-c",
    "gulp-uglify": "^3.0.2",
    "husky": "^3.1.0",
    "jest": "^24.9.0",
    "node-sass": "^7.0.0",
    "nodemon": "^1.19.4",
    "prettier": "^1.19.1",
    "rimraf": "^3.0.0",
    "stylelint": "^12.0.0",
    "stylelint-config-standard": "^19.0.0",
    "stylelint-scss": "^3.13.0",
    "supertest": "^4.0.2",
    "ts-jest": "24.1.0",
    "ts-loader": "^6.2.1",
    "ts-node": "8.5.2",
    "tsconfig-paths": "3.9.0",
    "tslint": "5.20.1",
    "typescript": "^3.7.3",
    "vinyl-source-stream": "^2.0.0",
    "wait-on": "^3.3.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "husky": {
    "hooks": {
      "pre-commit": "git add --all && npm run format && npm run lint &&  npm run clean && npm run build:production "
    }
  }
}
